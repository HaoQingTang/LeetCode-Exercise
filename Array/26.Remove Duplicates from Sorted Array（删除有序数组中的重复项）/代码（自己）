[C++]

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int a = 1;
        int b = 1;
        int n = nums.size();
        if (n == 0){
            return 0;
        }
        for(a=1; a<n; a++){
            if(nums[a] != nums[a-1]){
            nums[b] = nums[a];
            b = b+1;
        }
        }
        return b;
    }
};
